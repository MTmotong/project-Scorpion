<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<!-- START SNIPPET: xworkSample -->
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="ajax" extends="json-default">

		<action name="print" class="action.PrintAction" method="printcode">
			<result type="json"></result>
		</action>

		<action name="testRunTime" class="action.TestRunTime">
			<result name="success">testRunTime.jsp</result>
		</action>
		
		<action name="testA" class="test.TryAjax">
			<result>
				tryajax.jsp
			</result>
		</action>
		
		<action name="testAjax" class="test.TryAjax">
			<result type="json">
				
			</result>
		</action>
		<action name="load" class="action.LoadDir">
			<result name="success" type="redirect">
				/code
			</result>
			<result name="fail" type="redirect">
				/
			</result>
		</action>
		
		<action name="code">
			<result>
				/test.jsp
			</result>
		</action>
		
		<action name="loadfile" class="action.LoadFiles" method="loadfiles">
			<result type="json"></result>
		</action>
		
		<action name="printfilename" class="action.PrintFileName" method="execute">
			<result type="json">
			</result>
		</action>
		
		
	</package>
	
	
	
	<!-- <package name="ajax" extends="json-default">
		<action name="ajaxRequest" class="action.Hello">
			<result type="json"></result>
		</action>
	</package>
 -->
</struts>

<!-- END SNIPPET: xworkSample -->
